@model Teacher

<h2>Modifier le prof</h2>

<form asp-action="Update">
    <div class="form-group">
        <label asp-for="Firstname"></label>
        <input asp-for="Firstname" class="form-control" />
        <span asp-validation-for="Firstname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Lastname"></label>
        <input asp-for="Lastname" class="form-control" />
        <span asp-validation-for="Lastname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Age"></label>
        <input asp-for="Age" class="form-control" />
        <span asp-validation-for="Age" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Matter"></label>
        <input asp-for="Matter" class="form-control" />
        <span asp-validation-for="Matter" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Langage"></label>
        <input asp-for="Langage" class="form-control" />
        <span asp-validation-for="Langage" class="text-danger"></span>
    
  <button type="submit" class="btn btn-primary">Modifier le prof</button>
</form>



